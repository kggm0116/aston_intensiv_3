<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".launcher.LauncherActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_flags"
        style="@style/style_launcher_button"
        android:text="@string/label_flags"
        app:layout_constraintBottom_toTopOf="@id/button_images"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_images"
        style="@style/style_launcher_button"
        android:text="@string/label_images"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_flags"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>